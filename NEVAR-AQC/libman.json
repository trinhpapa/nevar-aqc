{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "Chart.js@2.7.3",
      "destination": "wwwroot/lib/Chart.js/",
      "files": [
        "Chart.min.js"
      ]
    },
    {
      "library": "@aspnet/signalr@1.1.2",
      "destination": "wwwroot/lib/aspnet/signalr/",
      "files": [
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "countup.js@2.0.4",
      "destination": "wwwroot/lib/countup.js/",
      "files": [
        "countUp.js"
      ]
    }
  ]
}